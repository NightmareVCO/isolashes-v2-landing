---
import ServiceContentCard from "../ServiceContentCard/ServiceContenCard.astro";

interface Props {
  services: {
    name: string;
  }[];
  index: number;
}

const { services, index } = Astro.props;
---

<ul>
  {
    services.map((service) => (
      <ServiceContentCard index={index} name={service.name} />
    ))
  }
</ul>
